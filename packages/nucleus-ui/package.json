{
  "name": "@sdh-project-services/nucleus-ui",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "run-s build:rimraf build:cpx build:tsc",
    "build:cpx": "cpx 'src/**/*.css' dist",
    "build:rimraf": "rimraf dist",
    "build:tsc": "tsc",
    "build-storybook": "build-storybook",
    "dev": "run-p dev:cpx dev:tsc",
    "dev:cpx": "cpx --watch 'src/**/*.css' dist",
    "dev:tsc": "tsc --watch",
    "postinstall": "yarn build",
    "storybook": "start-storybook -p 6006",
    "test": "jest"
  },
  "engines": {
    "node": ">= 12.0.0",
    "npm": "please-use-yarn"
  },
  "workspaces": {
    "nohoist": [
      "@storybook/addon-storyshots"
    ]
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "react": "^16.13.1",
    "react-hook-form": "^5.7.2",
    "react-icons": "^3.10.0",
    "react-select": "^3.1.0",
    "react-spinners": "^0.9.0",
    "react-tooltip": "^4.2.7"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-storyshots": "^5.3.19",
    "@storybook/addon-storysource": "^5.3.19",
    "@storybook/addon-viewport": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^5.3.19",
    "@tailwindcss/ui": "^0.3.0",
    "@types/react-select": "^3.0.14",
    "autoprefixer": "^9.8.4",
    "babel-loader": "^8.1.0",
    "cpx": "^1.5.0",
    "css-loader": "^3.6.0",
    "css-modules-typescript-loader": "^4.0.0",
    "fork-ts-checker-webpack-plugin": "^5.0.7",
    "jest": "^25.5.4",
    "npm-run-all": "^4.1.5",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "style-loader": "^1.2.1",
    "tailwindcss": "^1.4.6",
    "ts-jest": "25.5.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.8.3"
  }
}
